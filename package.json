{
	"name": "sp-vscode-assistant",
	"displayName": "sp-vscode-assistant",
	"description": "",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.45.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sp-vscode-assistant.uploadFiles",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sp-vscode-assistant.uploadFiles",
				"title": "Sync Files"
			},
			{
				"command": "sp-vscode-assistant.removeFiles",
				"title": "Remove Object on Storage"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "sp-vscode-assistant.uploadFiles",
					"when": "false"
				},
				{
					"command": "sp-vscode-assistant.removeFiles",
					"when": "false"
				}
			],
			"editor/title/context": [
				{
					"command": "sp-vscode-assistant.uploadFiles",
					"group": "spVscodeAssistant"
				},
				{
					"command": "sp-vscode-assistant.removeFiles",
					"group": "spVscodeAssistant"
				}
			],
			"explorer/context": [
				{
					"command": "sp-vscode-assistant.uploadFiles",
					"group": "spVscodeAssistant"
				},
				{
					"command": "sp-vscode-assistant.removeFiles",
					"group": "spVscodeAssistant"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/minio": "^7.0.6",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/request": "^2.48.5",
		"@types/request-promise-native": "^1.0.17",
		"@types/vscode": "^1.45.1",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"publisher": "woshiyanqinghao",
	"extensionKind": [
		"ui",
		"workspace"
	],
	"dependencies": {
		"@types/crypto-js": "^3.1.47",
		"ali-oss": "^6.10.0",
		"crypto-js": "^4.0.0",
		"minio": "^7.0.16",
		"request": "^2.88.2",
		"request-promise-native": "^1.0.9"
	}
}
